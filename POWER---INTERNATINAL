        if ($("#inquiryForm").length > 0) {

                $("#inquiryForm").validate({
                    rules: {
                        contact_name: {
                        required: true,
                        maxlength: 50
                        },
                        contact_email: {
                        required: true,
                        maxlength: 50,
                        email: true,
                        },  
                        contact_message: {
                        required: true,
                        maxlength: 300
                        },   
                    },
                    messages: {
                        contact_name: {
                        required: "Please enter name",
                        maxlength: "Your name maxlength is 50 characters."
                    },
                        contact_email: {
                        required: "Please enter valid email",
                        email: "Please enter valid email",
                        maxlength: "The email should be less than or equal to 50 characters",
                    },   
                     contact_message: {
                        required: "Please enter message",
                        maxlength: "Your message maxlength is 300 characters long."
                    },
                    },
                    submitHandler: function(form) {
                        
                       /* if (grecaptcha.getResponse(0) == ""){
                            alert("Please validate google recaptcha");
                        } else {*/
                              $.ajaxSetup({
                                headers: {
                                    'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                                }
                            });

                            $('#submit').html('Please Wait...');
                            $("#submit"). attr("disabled", true); 


                            $.ajax({
                                url: "https://aix-investments.com/store-data",
                                type: "POST",
                                data: $('#inquiryForm').serialize(),
                                dataType: "json",
                                success: function( response ) {
                                    $('#submit').html('Submit');
                                    $("#submit"). attr("disabled", false);
                                    //var returnedData = JSON.parse(response);
                                    if(response.success) {
                                        $("#exampleModalCenter").modal('show');
                                        document.getElementById("inquiryForm").reset();
                                        gtag('event', 'conversion', {'send_to': 'AW-610034682/zv6JCIC6y9kBEPrH8aIC'});

                                        linkedinpixel();
                                        uet_report_conversion();

                                        localStorage.setItem("formsubmitted", 'yes');
                                        twttr.conversion.trackPid('o6ogf', { tw_sale_amount: 0, tw_order_quantity: 0 }); 
                                    } else {
                                        alert(response.error);
                                    }
                                }
                            });
                       // }

                      
                    }
                })
            }


