             // Exit intent
            function addEvent(obj, evt, fn) {
                if (obj.addEventListener) {
                    obj.addEventListener(evt, fn, false);
                }
                else if (obj.attachEvent) {
                    obj.attachEvent("on" + evt, fn);
                }
            }

            // Exit intent trigger
            addEvent(document, 'mouseout', function(event) {

              //  if (evt.toElement == null && evt.relatedTarget == null ) {
                if(event.clientY <= 0 || event.clientX <= 0 || (event.clientX >= window.innerWidth || event.clientY >= window.innerHeight)){

                     var ab = $(".country_field").val();
                    $("#country_code").val(ab);

                    var storedValue = localStorage.getItem("formsubmitted");

                    console.log(event.fromElement);

                    if($(event.fromElement).hasClass('form-group')) {
                        return false;
                    }

                    if(!$('body').hasClass('modal-open') && storedValue != "yes") {
                       // $("#exampleModalCenter4").modal('show');
                        localStorage.setItem("formsubmitted", 'yes');
                    }
                };

            });


    </script>

